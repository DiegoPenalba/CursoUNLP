programa EjemploPiramide
procesos
  proceso Cuadrado1x1
  comenzar
    repetir 4
      mover
      derecha
  fin
areas
  ciudad: AreaC (1,1,100,100)
robots
  robot robot1
  variables
    Cant: numero
    Av: numero
  comenzar
    Av := 1
    Cant := 29
    repetir 15  {son 10 pisos}
      repetir Cant  
        {cantidad de cuadrados por piso}
        Cuadrado1x1
        Pos(PosAv + 1, PosCa)
      Cant := Cant - 2
      Av := Av + 1
      Pos(Av, PosCa + 1)
  fin
  
variables
  R_info: robot1
comenzar
  AsignarArea(R_info, ciudad)
  Iniciar(R_info, 1,1)
fin