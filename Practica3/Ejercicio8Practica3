{Programe al robot para que recorra la calle 2 hasta encontrar al menos 10 papeles. Pueden no haber 10 papeles.}

programa Ejercicio8Practica3 
areas
  ciudad: AreaC(1,1,100,100)
robots
  robot robot1
  variables
    papeles : numero
  comenzar
    papeles := 0
    Pos(1,34)
    derecha
    repetir 99
      mientras ((HayPapelEnLaEsquina) & (papeles <= 10))
        tomarPapel
        papeles := papeles + 1
      mover
    papeles := 0
    mientras ((HayPapelEnLaEsquina) & (papeles <= 10))
      tomarPapel
      papeles := papeles + 1
  fin
variables
  Rinfo: robot1
comenzar
  AsignarArea(Rinfo,ciudad)
  Iniciar(Rinfo,1,1)
fin